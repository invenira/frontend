{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:playwright": "vite --mode playwright",
    "build": "graphql-codegen --config codegen.ts && tsc -b && vite build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:e2e": "playwright test --config e2e/playwright.config.ts",
    "test:coverage": "vitest run --coverage",
    "codegen": "graphql-codegen --config codegen.ts",
    "all": "eslint . && prettier . --check && graphql-codegen --config codegen.ts && tsc -b && vite build && vitest run --coverage && playwright test --config e2e/playwright.config.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.114.15",
    "axios": "^1.7.9",
    "dompurify": "^3.2.4",
    "framer-motion": "^12.4.2",
    "graphql": "^16.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-oidc-context": "^3.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@eslint/js": "^9.19.0",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.50.1",
    "@semantic-release-plus/docker": "^3.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.0",
    "prettier": "^3.5.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
